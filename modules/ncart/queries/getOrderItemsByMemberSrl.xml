<query id="getOrderItemsByMemberSrl" action="select">
	<tables>
		<table name="ncart" />
	</tables>
	<columns>
		<column name="*" />
	</columns>
	<conditions>
		<condition operation="equal" column="member_srl" var="member_srl" notnull="notnull" />
		<condition operation="equal" column="module" var="module" pipe="and" />
		<condition operation="equal" column="item_srl" var="item_srl" pipe="and" />
		<condition operation="more" column="order_status" var="more_status" pipe="and" />
		<condition operation="less" column="order_status" var="less_status" pipe="and" />
	</conditions>
	<navigation>
		<index var="sort_index" default="cart_srl" order="desc" />
	</navigation>
</query>

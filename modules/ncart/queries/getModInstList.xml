<query id="getModInstList" action="select">
	<tables>
		<table name="modules" />
	</tables>
	<columns>
		<column name="*" />
	</columns>
	<conditions>
		<condition operation="equal" column="module" default="ncart" />
	</conditions>
	<navigation>
		<index var="sort_index" default="module_srl" order="desc" />
	</navigation>
</query>

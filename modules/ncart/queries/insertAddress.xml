<query id="insertAddress" action="insert">
	<tables>
		<table name="ncart_address" />
	</tables>
	<columns>
		<column name="address_srl" var="address_srl" filter="number" notnull="notnull" />
		<column name="member_srl" var="member_srl" notnull="notnull" />
		<column name="opt" var="opt" notnull="notnull" />
		<column name="title" var="title" notnull="notnull" />
		<column name="address" var="serialized_address" notnull="notnull" />
		<column name="default" var="default" default="N" />
		<column name="list_order" var="list_order" />
		<column name="regdate" var="regdate" default="curdate()" />
	</columns>
</query>

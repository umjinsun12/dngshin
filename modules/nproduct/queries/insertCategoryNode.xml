	<tables>
		<table name="nproduct_categories" />
	</tables>
	<columns>
		<column name="node_id" var="node_id" filter="number" notnull="notnull" />
		<column name="node_route" var="node_route" notnull="notnull" />
		<column name="node_route_text" var="node_route_text" notnull="notnull" />
		<column name="module_srl" var="module_srl" notnull="notnull" />
		<column name="category_name" var="category_name" notnull="notnull" />
		<column name="list_order" var="list_order" notnull="notnull" />
		<column name="regdate" var="regdate" default="curdate()" />
	</columns>
</query>

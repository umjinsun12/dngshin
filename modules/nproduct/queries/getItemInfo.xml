<query id="getItemInfo" action="select">
	<tables>
		<table name="nproduct_items" />
	</tables>
	<columns>
		<column name="*" />
	</columns>
	<conditions>
		<condition operation="equal" column="item_srl" var="item_srl" />
		<condition operation="equal" column="document_srl" var="document_srl" pipe="and" />
		<condition operation="equal" column="item_code" var="item_code" pipe="and" />
	</conditions>
</query>

<?xml version="1.0" encoding="UTF-8"?>
<skin version="0.2">
	<title xml:lang="ko">쇼핑몰</title>
	<title xml:lang="en">Shopping mall</title>
	<title xml:lang="jp">ショッピングモール</title>
	<title xml:lang="zh-TW">Shopping mall</title>		
	<description xml:lang="ko">쇼핑몰 스킨입니다</description>	
	<description xml:lang="en">Shopping mall skin</description>
	<description xml:lang="jp">ショッピングモールスキン</description>
	<description xml:lang="zh-TW">Shopping mall 版面</description>	
	<version>1.0</version>
	<date>2012-01-30</date>
	<link>http://store.nurigo.net</link>

	<author email_address="contact@nurigo.net" link="http://www.nurigo.net">
		<name xml:lang="ko">NURIGO</name>
	</author>

	<colorset>
		<color name="white">
			<title xml:lang="ko">하얀색(기본)</title>
			<title xml:lang="en">WHITE(Default)</title>
			<title xml:lang="jp">白色(基本)</title>
			<title xml:lang="zh-TW">白色（預設）</title>
		</color>
		<color name="black">
			<title xml:lang="ko">검정색</title>
			<title xml:lang="en">BLACK(Default)</title>
			<title xml:lang="jp">黒色(基本)</title>
			<title xml:lang="zh-TW">黑色（預設）</title>	
		</color>
	</colorset>

	<extra_vars>
		<group>
			<title xml:lang="ko">상세페이지</title>
			<title xml:lang="en">More details</title>
			<title xml:lang="jp">詳細</title>
			<title xml:lang="zh-TW">詳細頁版</title>			
			<var name="thumbnail_width" type="text">
				<title xml:lang="ko">썸네일 가로크기</title>
				<title xml:lang="en">Horizontal thumbnail size</title>
				<title xml:lang="jp">サムネイル横方向のサイズ</title>
				<title xml:lang="zh-TW">縮圖橫向尺寸</title>				
				<description xml:lang="ko">
					썸네일 가로크기 설정 (px 제외 입력)
				</description>
				<description xml:lang="en">
					Set the horizontal thumbnail size (Input number only,unit:pixel)
				</description>
				<description xml:lang="jp">
					サムネイル横方向のサイズを設定します。 (数字のみ入力,単位:pixel)
				</description>
				<description xml:lang="zh-TW">
					設定縮圖橫向尺寸 （只能輸入數字，單位：pixel 像素）
				</description>	
			</var>
			<var name="thumbnail_height" type="text">
				<title xml:lang="ko">썸네일 세로크기</title>
				<title xml:lang="en">Vertical thumbnail size</title>		
				<title xml:lang="jp">サムネイル縦方向のサイズ</title>
				<title xml:lang="zh-TW">縮圖垂直尺寸</title>				
				<description xml:lang="ko">
					썸네일 세로크기 설정 (px 제외 입력)
				</description>
				<description xml:lang="en">
					Set the vertical thumbnail size (Input number only,unit:pixel)
				</description>
				<description xml:lang="jp">
					サムネイル縦方向のサイズを設定します。 (数字のみ入力,単位:pixel)
				</description>
				<description xml:lang="zh-TW">
					設定縮圖垂直尺寸 （只能輸入數字，單位：pixel 像素）
				</description>	
			</var>
			<var name="thumbnail_type" type="select">
				<title xml:lang="ko">썸네일 생성 방법</title>
				<title xml:lang="en">Thumbnail creation</title>
				<title xml:lang="jp">サムネイル生成方法</title>
				<title xml:lang="zh-TW">建立縮圖</title>		
				<description xml:lang="ko">썸네일 생성 방법을 선택할 수 있습니다.</description>
				<description xml:lang="zh-TW">您可以選擇如何創建一個縮圖.</description>
				<options value="crop">
					<title xml:lang="ko">꽉 채우기</title>
					<title xml:lang="en">Full size</title>
					<title xml:lang="jp">フルサイズ</title>
					<title xml:lang="zh-TW">全尺寸</title>		
				</options>
				<options value="ratio">
					<title xml:lang="ko">비율 맞추기</title>
					<title xml:lang="en">Fit ratio</title>
					<title xml:lang="jp">フィット比率</title>
					<title xml:lang="zh-TW">完全比率</title>	
				</options>
			</var>
		</group>

		<group>
			<title xml:lang="ko">상품목록</title>
			<title xml:lang="en">Product List</title>
			<title xml:lang="jp">商品リスト</title>
			<title xml:lang="zh-TW">商品清單</title>			
			<var name="recommenditem_on" type="select">
				<title xml:lang="ko">추천상품 출력</title>
				<title xml:lang="en">Recommended Products</title>
				<title xml:lang="jp">お勧め商品</title>
				<title xml:lang="zh-TW">推薦商品</title>					
				<description xml:lang="ko">목록 상단에 추천상품을 진열합니다.</description>
				<options value="Y">
					<title xml:lang="ko">보이기</title>
					<title xml:lang="en">Show</title>
					<title xml:lang="jp">表示</title>
					<title xml:lang="zh-TW">顯示</title>						
				</options>
				<options value="N">
					<title xml:lang="ko">보이지 않기</title>
					<title xml:lang="en">Don't show</title>
					<title xml:lang="jp">表示しない</title>
					<title xml:lang="zh-TW">不顯示</title>						
				</options>
			</var>
			<var name="list_style" type="select">
				<title xml:lang="ko">상품목록 출력형식</title>
				<title xml:lang="en">Display type of product list</title>
				<title xml:lang="jp">商品リストの出力形式</title>
				<title xml:lang="zh-TW">商品清單列表輸出格式</title>								
				<description />
				<options value="list">
					<title xml:lang="ko">리스트</title>
					<title xml:lang="en">List</title>
					<title xml:lang="jp">リスト</title>
					<title xml:lang="zh-TW">清單</title>						
				</options>
				<options value="grid">
					<title xml:lang="ko">그리드</title>
					<title xml:lang="en">Grid</title>
					<title xml:lang="jp">グリッド </title>
					<title xml:lang="zh-TW">線格</title>						
				</options>
			</var>
			<var name="display_cart_buttons" type="select">
				<title xml:lang="ko">전체 장바구니 버튼</title>
				<title xml:lang="en">Whole cart button</title>
				<title xml:lang="jp">全てのカートボタン</title>
				<title xml:lang="zh-TW">全部購物車按鈕</title>								
				<description xml:lang="ko">전체 장바구니 버튼 출력 여부를 선택할 수 있습니다.</description>
				<description xml:lang="zh-TW">您可以選擇是否輸出所有購物車按鈕.</description>
				<options value="Y">
					<title xml:lang="ko">출력함</title>
					<title xml:lang="en">Show</title>
					<title xml:lang="jp">表示</title>
					<title xml:lang="zh-TW">不顯示</title>				
				</options>
				<options value="N">
					<title xml:lang="ko">출력하지 않음</title>
					<title xml:lang="en">Don't show</title>
					<title xml:lang="jp">表示しない</title>
					<title xml:lang="zh-TW">不顯示</title>						
				</options>
			</var>			
			<var name="list_count" type="text">
				<title xml:lang="ko">한페이지 출력상품 갯수</title>
				<title xml:lang="en">Number of product per 1 page</title>
				<title xml:lang="jp">1ページ当たり表示する商品数</title>
				<title xml:lang="zh-TW">每頁商品數量</title>					
				<description xml:lang="ko">
					미입력시 기본 30개 출력
				</description>
				<description xml:lang="en">
					如果留空白，會以預設值30個為每頁商品數量。
				</description>						
				<description xml:lang="jp">
					入力しないと30個表示
				</description>
				<description xml:lang="zh-TW">
					If left blank,Default output is 30.
				</description>					
			</var>
			<var name="list_thumbnail_width" type="text">
				<title xml:lang="ko">썸네일 가로크기</title>
				<title xml:lang="en">Horizontal thumbnail size</title>
				<title xml:lang="jp">サムネイル横方向のサイズ</title>
				<title xml:lang="zh-TW">縮圖橫向尺寸</title>		
				<description xml:lang="ko">
					썸네일 가로크기 설정 (px 제외 입력)
				</description>
				<description xml:lang="en">
					Set the horizontal thumbnail size (Input number only,unit:pixel)
				</description>
				<description xml:lang="jp">
					サムネイル横方向のサイズを設定します。 (数字のみ入力,単位:pixel)
				</description>
				<description xml:lang="zh-TW">
					設定縮圖橫向尺寸 （只能輸入數字，單位：pixel 像素）
				</description>	
			</var>
			<var name="list_thumbnail_height" type="text">
				<title xml:lang="ko">썸네일 세로크기</title>
				<title xml:lang="en">Vertical thumbnail size</title>		
				<title xml:lang="jp">サムネイル縦方向のサイズ</title>
				<title xml:lang="zh-TW">縮圖垂直尺寸</title>						
				<description xml:lang="ko">
					썸네일 세로크기 설정 (px 제외 입력)
				</description>
				<description xml:lang="en">
					Set the vertical thumbnail size (Input number only,unit:pixel)
				</description>
				<description xml:lang="jp">
					サムネイル縦方向のサイズを設定します。 (数字のみ入力,単位:pixel)
				</description>
				<description xml:lang="zh-TW">
					設定縮圖垂直尺寸 （只能輸入數字，單位：pixel 像素）
				</description>	
			</var>
			<var name="list_thumbnail_type" type="select">
				<title xml:lang="ko">썸네일 생성 방법</title>
				<title xml:lang="en">Thumbnail creation</title>
				<title xml:lang="jp">サムネイル生成方法</title>
				<title xml:lang="zh-TW">建立縮圖</title>						
				<description xml:lang="ko">썸네일 생성 방법을 선택할 수 있습니다.</description>
				<description xml:lang="zh-TW">您可以選擇如何創建一個縮圖.</description>
				<options value="crop">
					<title xml:lang="ko">꽉 채우기</title>
					<title xml:lang="en">Full size</title>
					<title xml:lang="jp">フルサイズ</title>
					<title xml:lang="zh-TW">全尺寸</title>							
				</options>
				<options value="ratio">
					<title xml:lang="ko">비율 맞추기</title>
					<title xml:lang="en">Fit ratio</title>
					<title xml:lang="jp">フィット比率</title>
					<title xml:lang="zh-TW">完全比率</title>						
				</options>
			</var>
			<var name="subject_cutsize" type="text">
				<title xml:lang="ko">상품명 출력크기</title>
				<description xml:lang="ko">
					상품명이 길어질 경우 출력크기를 적당히 조절하세요. 예로 10개의 글자를 출력할 경우 10을 입력합니다.
				</description>
			</var>
		</group>
		<group>
			<title xml:lang="ko">기타</title>
			<title xml:lang="en">etc.</title>
			<title xml:lang="jp">その他</title>
			<title xml:lang="zh-TW">其他</title>			
			<var name="store_top" type="textarea">
				<title xml:lang="ko">스토어 상단</title>
				<title xml:lang="en">Store Top</title>
				<title xml:lang="jp">ストア上部</title>
				<title xml:lang="zh-TW">商店頁面上部</title>				
				<description xml:lang="ko">
					스토어 상단 부분에 출력되는 부분입니다.
				</description>
				<description xml:lang="en">
					This is shown at the top of the store.
				</description>
				<description xml:lang="jp">
					ストアの上部に表示されます。
				</description>
				<description xml:lang="zh-TW">
					顯示在商店頁面上部.
				</description>				
			</var>
			<var name="store_bottom" type="textarea">
				<title xml:lang="ko">스토어 하단</title>
				<title xml:lang="en">Store Bottom</title>
				<title xml:lang="jp">ストア下部</title>
				<title xml:lang="zh-TW">商店頁面下部</title>					
				<description xml:lang="ko">
					스토어 하단 부분에 출력되는 부분입니다.
				</description>
				<description xml:lang="en">
					This is shown at the bottom of the store.
				</description>
				<description xml:lang="jp">
					ストアの下部に表示されます。
				</description>
				<description xml:lang="zh-TW">
					顯示在商店頁面下部.
				</description>
			</var>
		</group>
	</extra_vars>
</skin>

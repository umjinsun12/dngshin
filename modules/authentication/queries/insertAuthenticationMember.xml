<query id="insertAuthenticationMember" action="insert">
	<tables>
		<table name="authentication_member" />
	</tables>
	<columns>
		<column name="member_srl" var="member_srl" notnull="notnull" />
		<column name="authentication_srl" var="authentication_srl" notnull="notnull" />
		<column name="clue" var="clue" notnull="notnull" />
		<column name="authcode" var="authcode" notnull="notnull" />
		<column name="country_code" var="country_code" />
		<column name="regdate" var="regdate" default="curdate()" />
	</columns>
</query>

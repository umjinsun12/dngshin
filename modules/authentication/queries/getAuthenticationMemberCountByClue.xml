<query id="getAuthenticationMemberCountByClue" action="select">
	<tables>
		<table name="authentication_member" />
	</tables>
	<columns>
		<column name="COUNT(*)" alias="count" />
	</columns>
	<conditions>
		<condition operation="equal" column="clue" var="clue" notnull="notnull" />
	</conditions>
</query>

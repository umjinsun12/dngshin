<query id="getAdminPushList" action="select">
	<tables>
		<table name="androidpushapp_push" />
	</tables>
	<navigation>
		<index default="push_date" var="sort_index" order="order_type" />
		<list_count var="list_count" default="20" />
		<page_count var="page_count" default="10" />
		<page var="page" default="1" />
	</navigation>
</query>

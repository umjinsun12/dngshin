<query id="insertPlugin" action="insert">
    <tables>
        <table name="epay_plugins" />
    </tables>
    <columns>
        <column name="plugin_srl" var="plugin_srl" filter="number" notnull="notnull" />
        <column name="plugin" var="plugin" notnull="notnull" />
        <column name="title" var="title" notnull="notnull" />
        <column name="regdate" var="regdate" default="curdate()" />
    </columns>
</query>

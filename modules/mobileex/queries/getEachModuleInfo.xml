<query id="getEachModuleInfo" action="select">
    <tables>
        <table name="mobileex_config" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="module_srl" var="module_srl" notnull="notnull" filter="number"/>
    </conditions>
</query>